<%= simple_form_for(list) do |f| %>
  <%= f.input :name, class: "col-6 mb-3" %>
  <%= f.input :photo, as: :file, class: "col-6 mb-3" %>
  <div class="d-flex align-items-center justify-content-center gap">
    <%= link_to "Back", lists_path, class: "link-dark" %>
    <%= f.button :submit, class: "btn btn-dark" %>
  </div>
<% end %>